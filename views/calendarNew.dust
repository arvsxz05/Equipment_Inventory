		<script>
			$(document).ready(function() {
				$('#calend').fullCalendar({
					events: 'http://172.16.15.130:3000/events'
				})
			
			});
		</script>	
</head>
<body>

	<div class="box span12">
		<div class="box-header" data-original-title="">
			<h2><i class="halflings-icon calendar"></i><span class="break"></span>Calendar</h2>
		</div>
	  	<div class="box-content">
			<div id="external-events" class="span3 hidden-phone hidden-tablet"></div>
			<div id='calend'></div>	
			<div class="clearfix"></div>
		</div>
	</div>

	<div class="box span6">
		<div class="box-header" style="margin-bottom:10px" data-original-title>
			<h2><i class="halflings-icon search"></i><span class="break"></span>Set Schedule</h2>
		</div>
		<div class="box-content">
			<form method="post" action="/setSchedule" class="form-horizontal">
				<fieldset>
				  	<div class="control-group">
						<label class="control-label" for="condition">Process</label>
						<div class="controls">
						  	<select name="title" id="condition" data-rel="chosen">
								<option value="Inventory">Inventory</option>
								<option value="Disposal">Disposal</option>
						  	</select>
						</div>
					</div>
					<div class="control-group">
				 		<label class="control-label" for="start">Start</label>
						<div class="controls" id="ReportDate">
			                 <div id="start">
			                  	<input id="date1" name="date1" accesskey="r" readonly="true" class="form-control datepicker" placeholder="Select Date" ng-required="true" ng-model="SelectedReportDate" type="text" required="required" data-ng-animate="2" onchange="checker()">
			                 </div>
			                 <span class="input-group-addon text-pointer">
			                  	<span class="glyphicon glyphicon-calendar"></span>
			                 </span>
		                </div>
					</div>

					<div class="control-group">
				 		<label class="control-label" for="end">End</label>
						<div class="controls" id="ReportDate">
			                 <div id="end">
			                  	<input id="date2" name="date2" accesskey="r" readonly="true" class="form-control datepicker" placeholder="Select Date" ng-required="true" ng-model="SelectedReportDate" type="text" required="required" data-ng-animate="2" disabled>
			                 </div>
			                 <span class="input-group-addon text-pointer">
			                  	<span class="glyphicon glyphicon-calendar"></span>
			                 </span>
		                </div>
					</div>
					 <input type="submit" class="btn btn-meduim btn-info" value="Save Changes" style="margin-left: 320px;" />
				  	<button class="btn btn-meduim">Cancel</button>
				</fieldset>
			</form>
		</div>
	</div>

		<!-- start: JavaScript-->

		

	<!-- end: JavaScript-->
</html>